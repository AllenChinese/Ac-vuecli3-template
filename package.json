{
  "name": "vue-cli-template",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:prod": "vue-cli-service --mode=production build",
    "build:test": "vue-cli-service --mode=development build",
    "lint": "vue-cli-service lint",
    "watch": "vue-cli-service build --watch"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "core-js": "^2.6.5",
    "element-ui": "^2.3.6",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.19.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tar": ">=4.4.2",
    "terser-webpack-plugin": "^2.3.1",
    "vue-eslint-parser": "^6.0.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.29.6",
    "serialize-javascript": ">=2.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "main": "index.js",
  "repository": "git@github.com:AllenChinese/vue-cli-template.git",
  "author": "AllenChinese <13402202685@163.com>",
  "license": "MIT"
}
